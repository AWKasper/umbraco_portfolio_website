@using Umbraco.Cms.Web.Common.PublishedModels;
@model Portfolio.Core.Models.ViewModels.MainViewModel

@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@Model.PageTitle</title>
    <meta name="og:title" content="@Model.PageTitle">
    @{ var url = "https://ak-portfolio.azurewebsites.net" + @Model.Content.Url();}
    <meta name="og:url" content="@url">
    <meta name="og:site_name" content="A. Kasper">
    <meta name="og:type" content="website">
    
    <!--other metadata and stylesheets that can be added in templates -->
    @await RenderSectionAsync("meta", required: false)

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="/css/Main.css"/>
</head>
<body>

<!--nav-bar -->
@await Html.PartialAsync("Navigation")

@RenderBody()

<!--scripts -->
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.min.js" integrity="sha384-QJHtvGhmr9XOIpI6YVutG+2QOK9T+ZnN4kzFN1RtK3zEFEIsxhlmWl5/YESvpZ13" crossorigin="anonymous"></script>
</body>
</html>